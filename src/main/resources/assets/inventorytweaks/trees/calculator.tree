				</pickaxe>
			</miningTool>
			<diggingTool treeOrder="30">
				<shovel class="Item" data="{ToolClass : &quot;shovel&quot;}" mergeChildren="true" treeOrder="20">
					<itemElectricShovel id="calculator:electricshovel" treeOrder="170"/>
					<itemEndForgedShovel id="calculator:endforgedshovel" treeOrder="210"/>
					<itemEnrichedGoldShovel id="calculator:enrichedgoldshovel" treeOrder="230"/>
