						<swordDamage4 treeOrder="130">
							<itemEnrichedGoldSword id="calculator:enrichedgoldsword" treeOrder="80"/>
						</swordDamage4>
					</sword>
				</sword>
			</meleeWeapon>
		</weapon>
		<tool treeOrder="20">
			<miningTool treeOrder="20">
				<pickaxe class="Item" data="{ToolClass : &quot;pickaxe&quot;}" mergeChildren="true" treeOrder="20">
					<CobaltPickLevel treeOrder="80">
						<itemElectricPickaxe id="calculator:electricpickaxe" treeOrder="50"/>
						<itemEndForgedPickaxe id="calculator:endforgedpickaxe" treeOrder="80"/>
					</CobaltPickLevel>
					<ObsidianLevel treeOrder="90">
						<itemEnrichedGoldPickaxe id="calculator:enrichedgoldpickaxe" treeOrder="100"/>
						<itemFireDiamondPickaxe id="calculator:firediamondpickaxe" treeOrder="110"/>
						<itemFlawlessDiamondPickaxe id="calculator:flawlessdiamondpickaxe" treeOrder="120"/>
						<itemWeakenedDiamondPickaxe id="calculator:weakeneddiamondpickaxe" treeOrder="240"/>
					</ObsidianLevel>
					<DiamondPickLevel treeOrder="100">
						<itemRedstonePickaxe id="calculator:redstonepickaxe" treeOrder="330"/>
