<!-- Note: 'stuff' matches everything, even items that don't appear here -->
<stuff treeVersion="1.12.2">
	<equipment treeOrder="10">
		<weapon treeOrder="10">
			<meleeWeapon treeOrder="10">
				<sword treeOrder="10">
					<sword class="ItemSword" data="{toolClass : &quot;&quot;}" mergeChildren="true" treeOrder="110">
						<swordDamage20 treeOrder="20">
							<itemEndForgedSword id="calculator:endforgedsword" treeOrder="10"/>
						</swordDamage20>
						<swordDamage14 treeOrder="40">
							<itemElectricSword id="calculator:electricsword" treeOrder="10"/>
						</swordDamage14>
						<swordDamage11 treeOrder="60">
							<itemFireDiamondSword id="calculator:firediamondsword" treeOrder="10"/>
						</swordDamage11>
						<swordDamage9 treeOrder="80">
							<itemFlawlessDiamondSword id="calculator:flawlessdiamondsword" treeOrder="70"/>
						</swordDamage9>
						<swordDamage7 treeOrder="100">
							<itemWeakenedDiamondSword id="calculator:weakeneddiamondsword" treeOrder="120"/>
						</swordDamage7>
						<swordDamage6 treeOrder="110">
							<itemRedstoneSword id="calculator:redstonesword" treeOrder="230"/>
							<itemReinforcedIronSword id="calculator:reinforcedironsword" treeOrder="240"/>
						</swordDamage6>
						<swordDamage5 treeOrder="120">
							<itemReinforcedSword id="calculator:reinforcedsword" treeOrder="70"/>
