				<itemRubySaw id="projred|exploration:projectred.exploration.sawruby"/>
				<itemSapphireSaw id="projred|exploration:projectred.exploration.sawsapphire"/>
			</saws>
		</tool>
		<armor>
			<helmet class="ItemArmor" data="{ArmorType : &quot;head&quot;}">
				<helmet3>
					<itemPeridotHelmet id="projred|exploration:projectred.exploration.peridothelmet"/>
