				</pickaxe>
			</miningTool>
			<diggingTool>
				<shovel class="Item" data="{ToolClass : &quot;shovel&quot;}" mergeChildren="true">
					<itemPeridotShovel id="projred|exploration:projectred.exploration.shovelperidot"/>
					<itemRubyShovel id="projred|exploration:projectred.exploration.shovelruby"/>
					<itemSapphireShovel id="projred|exploration:projectred.exploration.shovelsapphire"/>
				</shovel>
			</diggingTool>
			<choppingTool>
				<axe class="Item" data="{ToolClass : &quot;axe&quot;}" mergeChildren="true">
					<itemPeridotAxe id="projred|exploration:projectred.exploration.axeperidot"/>
					<itemRubyAxe id="projred|exploration:projectred.exploration.axeruby"/>
					<itemSapphireAxe id="projred|exploration:projectred.exploration.axesapphire"/>
				</axe>
			</choppingTool>
			<farmingTool>
				<sickle class="Item" data="{ToolClass : &quot;sickle&quot;}" mergeChildren="true">
