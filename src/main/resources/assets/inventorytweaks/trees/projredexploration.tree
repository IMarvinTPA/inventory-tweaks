					<itemSapphireSickle id="projred|exploration:projectred.exploration.sicklesapphire"/>
					<itemIronSickle id="projred|exploration:projectred.exploration.sickleiron"/>
					<itemStoneSickle id="projred|exploration:projectred.exploration.sicklestone"/>
					<itemGoldenSickle id="projred|exploration:projectred.exploration.sicklegold"/>
					<itemWoodenSickle id="projred|exploration:projectred.exploration.sicklewood"/>
				</sickle>
				<hoe class="Item" data="{ToolClass : &quot;hoe&quot;}" mergeChildren="true">
					<itemPeridotHoe id="projred|exploration:projectred.exploration.hoeperidot"/>
					<itemRubyHoe id="projred|exploration:projectred.exploration.hoeruby"/>
					<itemSapphireHoe id="projred|exploration:projectred.exploration.hoesapphire"/>
				</hoe>
			</farmingTool>
			<saws class="Item" data="{ToolClass : &quot;saw&quot;}">
				<itemGoldenSaw id="projred|exploration:projectred.exploration.sawgold"/>
