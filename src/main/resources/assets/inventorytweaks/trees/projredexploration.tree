<!-- Note: 'stuff' matches everything, even items that don't appear here -->
<stuff treeVersion="1.12.2">
	<equipment>
		<weapon>
			<meleeWeapon>
				<sword>
					<sword class="ItemSword" data="{toolClass : &quot;&quot;}" mergeChildren="true">
						<swordDamage7>
							<itemRubySword id="projred|exploration:projectred.exploration.swordruby"/>
							<itemSapphireSword id="projred|exploration:projectred.exploration.swordsapphire"/>
						</swordDamage7>
						<swordDamage6>
							<itemPeridotSword id="projred|exploration:projectred.exploration.swordperidot"/>
						</swordDamage6>
					</sword>
				</sword>
			</meleeWeapon>
		</weapon>
		<tool>
			<miningTool>
				<pickaxe class="Item" data="{ToolClass : &quot;pickaxe&quot;}" mergeChildren="true">
					<DiamondPickLevel>
						<itemPeridotPickaxe id="projred|exploration:projectred.exploration.pickaxeperidot"/>
						<itemRubyPickaxe id="projred|exploration:projectred.exploration.pickaxeruby"/>
						<itemSapphirePickaxe id="projred|exploration:projectred.exploration.pickaxesapphire"/>
