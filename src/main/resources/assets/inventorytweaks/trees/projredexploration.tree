				</leggings6>
			</leggings>
			<boots class="ItemArmor" data="{ArmorType:&quot;feet&quot;}">
				<boots3>
					<itemRubyBoots id="projred|exploration:projectred.exploration.rubyboots"/>
					<itemSapphireBoots id="projred|exploration:projectred.exploration.sapphireboots"/>
					<itemPeridotBoots id="projred|exploration:projectred.exploration.peridotboots"/>
