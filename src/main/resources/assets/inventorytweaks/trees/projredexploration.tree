	</equipment>
	<block>
		<manufacturedBlock>
			<stoneWall>
				<itemProjectRedWall id="projred|exploration:projectred.exploration.stonewalls"/>
			</stoneWall>
		</manufacturedBlock>
	</block>
	<item>
		<utility>
			<storage>
				<barrels>
					<itemProjRed_Exploration_projectred_exploration_barrel id="projred|exploration:projectred.exploration.barrel"/>
				</barrels>
				<portableStorage>
					<itemPRBackpack id="projred|exploration:projectred.exploration.backpack"/>
				</portableStorage>
			</storage>
		</utility>
