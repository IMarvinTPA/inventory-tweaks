					<itemSapphireHelmet id="projred|exploration:projectred.exploration.sapphirehelmet"/>
				</helmet3>
			</helmet>
			<chestplate class="ItemArmor" data="{ArmorType:&quot;chest&quot;}">
				<chestplateArmor>
					<chestplate8>
						<itemPeridotChestplate id="projred|exploration:projectred.exploration.peridotchestplate"/>
